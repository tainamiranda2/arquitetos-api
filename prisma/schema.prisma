// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
 provider = "postgresql"
  url      = "postgresql://admin:admin@localhost:5432"
}

model arquiteto{
 id Int @id @default(autoincrement())
 nome String
email  String
telefone Int
genero  String
idade Int
senha Int
papel  String
}
model servico{
   id Int @id @default(autoincrement())
   nome  String
   descricao String
   estado	 String
   prazoI  String
   	prazoF  String
    arquiteto_id Int
    cliente_id Int
}
model cliente{
   id Int @id @default(autoincrement())
   nome String
email  String
telefone Int
genero  String
idade Int
senha Int
papel  String
}